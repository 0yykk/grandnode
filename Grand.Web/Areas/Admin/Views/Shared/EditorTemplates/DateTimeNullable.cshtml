@model DateTime?
@inject Grand.Services.Localization.ILanguageService languageService;

<input id="@Html.IdForModel()" name="@Html.NameForModel()" value="@(Model > DateTime.MinValue || Model.HasValue ? Model.Value.ToString() : null)" />
<script>
    $(document).ready(function () {
        $("#@Html.IdForModel()").kendoDateTimePicker({
            culture: "en-US"
        });
    });
</script>